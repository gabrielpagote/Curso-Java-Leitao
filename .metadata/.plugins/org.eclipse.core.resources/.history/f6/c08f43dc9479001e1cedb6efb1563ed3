package pacote;

import javax.swing.*;

public class classe {
 public static void main(String[] args) {
	JFrame jframe = new JFrame();
	String getMessage = 	JOptionPane.showInputDialog(jframe, "Quem é o amor da sua vida?");
	
	if (getMessage === "Juliana Moura Mendonça") {
		JOptionPane.showMessageDialog(jframe, "Muito bem  issso aí! <3");
	}
	else {
		JOptionPane.showMessageDialog(jframe, "Resposta incorreta, repense seus conceitos!");
		
	}
	
	
}
}
