package fundamentos;

public class Inferencia {
	
	public static void main(String[] args) {
		
		double a = 4.5;
		System.out.println(a);
		
		//a linguagem irá determinar o tipo da variável com base no que escrevemos "var"
		var b = 4.5;
		System.out.println(b);
		
		var c = "Texto";
		c = "Outro texto";
	}
}
